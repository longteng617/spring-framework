<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id = "apple" class="org.springframework.debug.method.overrides.Apple" scope="prototype"></bean>

	<bean id = "fruitPlate" class="org.springframework.debug.method.overrides.FruitPlate">
		<lookup-method name="getFruit"></lookup-method>
